
!----------------- Times -----------------
TSTART = 0.01;
TEND = 0.05;
TINIT = 0.01;

DROUT = 1.d-4;
DPOUT = 1.d-4;
DTOUT = 1.d-5;


CF1 = 1.d-4;
CF2 = 0.01;
CF3 = 0.011;

!----------------- Electrons parametrs -----------------

NE:AS;
NE = (6.3 - 0.3)*(1-(RHO/ROC)**4) + 0.6;

TE:AS;
TE = (1.54 - 0.15)*(1-(RHO/ROC)**2) + 0.15;
!----------------- Ion parametrs -----------------

TI:AS; 
TI = (4.5 - 0.4)*(1-(RHO/ROC)**2)**2 + 0.4;

!----------------- Impurities -----------------
AMAIN = AMJ;
ZMAIN = ZMJ;

AIM1 = 12;
ZIM1 = 6;


ZEF = (3.4 - 1.4)*(1-(RHO/ROC)**4) + 1.4;

!Its densities

NIZ1 = NE*(ZEF-ZMJ)/(ZIM1-ZMJ)/ZIM1;

NI = NE*(ZIM1-ZEF)/(ZIM1-ZMJ)/ZMJ;


NDEUT = NI - NIZ1;

!----------------- Plasma parametrs -----------------

!Current densities

CU:EQ;
CU = CNHH;

HC = HCSA; DC = DCSA; XC = XCSA;
CC = CNSA; CD = CUBM + 0.*CULH;

!----------------- Equilibrium solver -----------------

NBND = 100;
NEQUIL = 41;

!----------------- LHCD & DrivenCurrent -----------------

QLH = 0.001;

debug:0.0001:TINIT::;
saveprofiles::CF2:CF3:;
!lhcd2017(PELH):CF1:CF2:CF3:;
!drivencurrent(CULH,CAR32,CU)::CF2:CF3:;

CD = CULH;
CCD1 = VINT(PELHB);
CCD2 = IINT(CULHB);

CV10 =  IINT(CU) - IINT(CUBS) - CCD2;
CV11 =  IINT(CUBS);
!----------------- Auxiliary quantities -----------------

VTOR = 150; VPOL = VPSWW;
ER = BTOR*(FRS*MU*VTOR/RTOR+VDIA-VPOL);

!----------------- Radial output -----------------
Te\TE; Zeff\ZEF; Jpar\CU; shir\SHEAR;
Ne\NE; Elong\ELON; Cond\CC; PV\VP;
Ti\TI; E\UPL/RTOR/GP2; Tria\TRIA; q\1./MU;
Ni\NDEUT; Shift\SHIF; Jbs\CUBS; eps\AMETR/RTOR;
!----------------- Time output -----------
betp_BETAJB;













