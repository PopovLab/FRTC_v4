
!----------------- Times -----------------
TSTART = 0.01;
TEND = 0.011;
TINIT = 0.01;

DROUT = 1.d-4;
DPOUT = 1.d-4;
DTOUT = 1.d-5;
DSTART = 0.01;

CF1 = 1.d-4;
CF2 = 0.01;
CF3 = 0.011;

!----------------- Electrons parametrs -----------------

NE:AS;
!NE = 2*NEX;
NE = (5.0 - 1.0)*(1-(RHO/ROC)**2) + 1;
!NE = (2.0 - 0.003)*(1-(RHO/ROC)**2) + 0.003;

TE:AS;
!TE = 2*TEX;
TE = (5.0 - 0.01)*(1-(RHO/ROC)**4) + 0.01;
!----------------- Ion parametrs -----------------

TI:AS; 
TI = 0.8*TE;

!----------------- Impurities -----------------

AMAIN = AMJ;
ZMAIN = ZMJ;

ZEF = 1.0;

NI = NE;
NHYDR = NI;

!----------------- Plasma parametrs -----------------
NEOCL(100)<:;
!Current densities

CU:EQ;
CU = 0.01;
CC="work(j,303)";
CUBS="work(j,301)";

!----------------- Equilibrium solver -----------------

NBND = 100;
NEQUIL = 41;

!----------------- LHCD & DrivenCurrent -----------------

QLH = 0.001;

debug:0.0001:DSTART::;
saveprofiles::CF2:CF3:;
lhcd2017(PELH):CF1:CF2:CF3:;
drivencurrent(CULH,CAR32,CU)::CF2:CF3:;

CD = CULH;
CCD1 = VINT(PELHB);
CCD2 = IINT(CULHB);

CV10 =  IINT(CU) - IINT(CUBS) - CCD2;
CV11 =  IINT(CUBS);
!----------------- Auxiliary quantities -----------------

!----------------- Radial output -----------------
Te\TE; Jneo\(CU-CUBS-CD); Jpar\CU; shir\SHEAR;
Ne\NE\2; Elong\ELON; Cond\CC; Poh\POH;
Ti\TI; E\UPL/RTOR/GP2; Jlh\CULH; q\1./MU;
Ni\NHYDR; Shift\SHIF; Jbs\CUBS; Plh\PELH;
!----------------- Time output --------------
q0_1./MU(0); q1_1./MU(ROC);
Ibs_CV11; Iohm_CV10; Ilh_CCD2; Itot_CV10+CCD2+CV11;
Qlh_QLH; Plh_CCD1;











